{
    "name": "logcomex/php-utils",
    "description": "PHP Utils for Lumen and Laravel projects",
    "keywords": [],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "laravel/lumen-framework": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Logcomex\\PhpUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
